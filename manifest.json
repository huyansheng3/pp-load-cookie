{
  "name": "导入cookie",
  "version": "0.0.1",
  "description": "一键导入多个cookie，支持切换不同的cookie",
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "icons": {
    "16": "cookie.png",
    "48": "cookie.png",
    "128": "cookie.png"
  },
  "browser_action": {
    "default_icon": "cookie.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "manifest_version": 2
}